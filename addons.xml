<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.eyepeatvinstall" name="EyePeaTV Installer Repo" version="18.0.9" provider-name="EyePeaTV">
	<extension point="xbmc.addon.repository" name="EyePeaTV Repo">
			<info compressed="false">https://raw.githubusercontent.com/biglad/eptvinstall/master/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/biglad/eptvinstall/master/addons.xml.md5</checksum>
			<datadir zip="true">https://github.com/biglad/eptvinstall/raw/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary></summary>
		<description>EyePeaTV Repo</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.vistatv-installer"
    name="!!! EyePeaTV Build Installer !!!" version="100.0.5" provider-name="Biglad">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script" library="addon.py">
     <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup" />
  <extension point="xbmc.addon.metadata">
<language></language>
    <platform>all</platform>
    <license>EPTV 2018</license>
    <forum>EPTV 2018</forum>
    <source></source>
  </extension>
</addon>
</addons>
